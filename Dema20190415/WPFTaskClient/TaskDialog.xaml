<Window x:Class="WPFTaskClient.TaskDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTaskClient"
        mc:Ignorable="d"
        Title="TaskDialog" Height="202" Width="434" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Label x:Name="label" Content="Tytuł" HorizontalAlignment="Left" Margin="44,33,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Data" HorizontalAlignment="Left" Margin="44,74,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="titleBox" HorizontalAlignment="Left" Height="23" Margin="169,33,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="219"/>
        <Button x:Name="okButton" Content="Ok" HorizontalAlignment="Left" Margin="85,124,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="OkButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="249,124,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>
        <DatePicker x:Name="dueDatePicker" HorizontalAlignment="Left" Margin="169,76,0,0" VerticalAlignment="Top" Width="219"/>

    </Grid>
</Window>
